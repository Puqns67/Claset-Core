{
    "Format": {
        "Perfix": "[{Name}]",
        "Value": "{Value}",
        "Time": "%Y/%m/%d_%H:%M:%S",
        "FullLog": "[{Time}]{{Type}}{Perfixs}: {Text}."
    },
    "ProgressiveWrite": true,
    "Debug": true,
    "Types": [
        "INFO",
        "WARN",
        "DEBUG",
        "ERROR"
    ],
    "OldLogProcess": {
        "Type": "Delete",
        "Types": [
            "Delete",
            "Archive",
            "Archive + Delete"
        ],
        "Settings": {
            "Delete": {
                "MaxKeepFile": 3
            },
            "Archive": {
                "ArchiveFileName": "Claset-Log-Archive.lzma",
                "MaxKeepFile": 3
            },
            "Archive + Delete": {
                "MaxKeepFile": 30
            }
        }
    }
}